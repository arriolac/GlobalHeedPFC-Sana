<Procedure title="Hypertension" author="Partners For Care">

    <Page>
        <Element type="MULTI_SELECT" id="hypertensionHistory"
                concept="HYPERTENSION HISTORY"
                question="History of:"
                answer=""
                choices="Smoking,Alcohol Intake,Familial History of Hypertension" />
    </Page>

    <Page>
        <ShowIf>
            <Criteria type="EQUALS" id="hypertensionHistory" value="Familial History of Hypertension"/>
        </ShowIf>
        <Element type="ENTRY" id="medicationList"
                concept="MEDICATION LIST"
                question="Enter medications:" answer="" />
    </Page>

    <Page>
        <Element type="MULTI_SELECT" id="hypertensionSymptoms"
                concept="HYPERTENSION SYMPTOMS"
                question="Enter complaints:"
                answer=""
                choices="Headache,Chest Pains,Visual Disturbance,Dizziness,Other Complaints" />
    </Page>

    <Page>
        <Element type="ENTRY" id="duration" 
                concept="COMPLAINT DURATION" 
                numeric="DIALPAD"
                question="Enter Complaint Duration in Days:" answer=""/>
    </Page>

    <Page>
        <Element type="GPS" id="gps"
                concept="GPS"
                question="Record Your Position" answer=""/>
    </Page>
</Procedure>
